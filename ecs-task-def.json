{
  "family": "task-scraper",
  "containerDefinitions": [
    {
      "name": "container-scraper",
      "image": "REPLACED_BY_GITHUB_ACTION",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http",
          "name": "scraper-port-80"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/task-scraper",
          "awslogs-create-group": "true",
          "awslogs-region": "eu-north-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "environment": []
    }
  ],
  "executionRoleArn": "arn:aws:iam::509399624827:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
